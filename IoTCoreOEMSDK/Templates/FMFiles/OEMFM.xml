<?xml version="1.0" encoding="utf-8"?>
<FeatureManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.microsoft.com/embedded/2004/10/ImageUpdate">
  <BasePackages>
  <!-- List OEM packages that are to be included by default here -->
    <PackageFile Path="%PKG_OUTPUT%" Name="OEMName.Registry.SubName.cab"/>
  </BasePackages>
  <Features>
  <!-- List OEM packages that are optional here, specify a feature id to include this in OEMInput file -->
    <OEM>
      <PackageFile Path="%PKG_OUTPUT%" Name="OEMName.Appx.SubName.cab">
        <FeatureIDs>
          <FeatureID>OEMApps</FeatureID>
        </FeatureIDs>
      </PackageFile>
    </OEM>
    <OEMFeatureGroups />
  </Features>
</FeatureManifest>