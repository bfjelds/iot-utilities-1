      </PackageFile>
      <PackageFile Path="$(mspackageroot)\Retail\$(cputype)\$(buildtype)" Name="Intel.MBM.I2C.cab" >
        <FeatureIDs>
          <FeatureID>MBM_DRIVERS</FeatureID>
