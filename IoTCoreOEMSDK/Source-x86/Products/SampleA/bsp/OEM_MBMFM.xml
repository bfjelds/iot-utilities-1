  <DeviceLayoutPackages>
    <PackageFile SOC="MBM" Path="$(mspackageroot)\Retail\$(cputype)\$(buildtype)" Name="Intel.MBM.DeviceLayout.cab" FeatureIdentifierPackage="true"/>
