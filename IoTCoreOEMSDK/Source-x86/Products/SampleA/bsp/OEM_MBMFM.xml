      </PackageFile>
      <PackageFile Path="$(mspackageroot)\Retail\$(cputype)\$(buildtype)" Name="Intel.MBM.SPI.cab" >
        <FeatureIDs>
          <FeatureID>MBM_DRIVERS</FeatureID>
        </FeatureIDs>
      </PackageFile>
      <PackageFile Path="$(mspackageroot)\Retail\$(cputype)\$(buildtype)" Name="Intel.MBM.GRFX.cab" >
