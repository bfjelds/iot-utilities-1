<?xml version="1.0" encoding="utf-8"?>
<OEMInput xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.microsoft.com/embedded/2004/10/ImageUpdate">
  <Description>Windows 10 IoT Core MBM Test VHD generation for x86.fre with build number 20150824-1849 by wincbld</Description>
  <SOC>MBM</SOC>
  <SV>Intel</SV>
  <Device>MBM</Device>
  <ReleaseType>Test</ReleaseType>
  <BuildType>fre</BuildType>
  <SupportedLanguages>
    <UserInterface>
      <Language>en-us</Language>
    </UserInterface>
    <Keyboard>
      <Language>en-us</Language>
    </Keyboard>
    <Speech>
      <Language>en-us</Language>
    </Speech>
  </SupportedLanguages>
  <BootUILanguage>en-us</BootUILanguage>
  <BootLocale>en-us</BootLocale>
  <Resolutions>
    <Resolution>1024x768</Resolution>
  </Resolutions>
  <AdditionalFMs>
    <!-- Including BSP feature manifest -->
	<AdditionalFM>%PRODSRC_DIR%\bsp\OEM_MBMFM.xml</AdditionalFM>
	<!-- Including OEM feature manifest -->
	<AdditionalFM>%PRJ_DIR%\Packages\OEMFM.xml</AdditionalFM>
		<!-- Including the test features -->
	<AdditionalFM>%KITSROOT%\FMFiles\x86\IoTUAPNonProductionPartnerShareFM.xml</AdditionalFM>
  </AdditionalFMs>
  <Features>
   <Microsoft> 
      <Feature>IOT_EFIESP</Feature> 
      <Feature>IOT_EFIESP_BCD</Feature> 
      <Feature>IOT_DMAP_DRIVER</Feature> 
      <Feature>IOT_POWER_SETIINGS</Feature>
	  <!-- Following two required for Appx Installation -->
	  <Feature>IOT_UAP_OOBE</Feature> 
	  <Feature>IOT_APP_TOOLKIT</Feature> 
      <!-- for Connectivity -->
	  <Feature>IOT_TOOLKIT</Feature> 
	  <Feature>IOT_WEBB_EXTN</Feature>
      <Feature>IOT_POWERSHELL</Feature>
	  <Feature>IOT_NETCMD</Feature> 	  
	  <Feature>IOT_SSH</Feature> 
	  <Feature>IOT_SIREP</Feature>
	  <!-- Enabling Test images -->
	  <Feature>IOT_DISABLE_UMCI</Feature> 
	  <Feature>IOT_ENABLE_TESTSIGNING</Feature>		  
    </Microsoft>
    <OEM>
	  <!-- Inclide BSP Features -->
      <Feature>MBM_DEVICE_TARGETINGINFO</Feature> 
      <Feature>PLACEHOLDER_FEATURE</Feature> 
      <Feature>MBM_DRIVERS</Feature> 
	  <!-- Include OEM features -->
	  <Feature>Contoso_AppxMain</Feature>
	  <Feature>Contoso_CustomCmd</Feature>
	  <Feature>Contoso_ProvAuto</Feature> 
   </OEM> 
  </Features>
  <PackageFiles />
 <Product>Windows 10 IoT Core</Product>
</OEMInput>