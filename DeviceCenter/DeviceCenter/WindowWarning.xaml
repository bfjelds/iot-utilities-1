<Window x:Class="DeviceCenter.WindowWarning"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeviceCenter"
        xmlns:props="clr-namespace:DeviceCenter.Strings"
        mc:Ignorable="d"
        Title="{x:Static props:Strings.AppNameDisplay}" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" Width="494.667" Height="229.667" ShowInTaskbar="False">
    <DockPanel Background="WhiteSmoke">
        <TextBlock DockPanel.Dock="Top" x:Name="labelMessageHeader" Margin="10" FontSize="40" FontFamily="Segoe UI SemiLight" Text="Title" />
        <StackPanel  DockPanel.Dock="Bottom" x:Name="grdContinueCancel" Orientation="Horizontal" HorizontalAlignment="Right" Margin="10">
            <Button x:Name="btnContinue" x:Uid="ViewDevicesButton" IsDefault="True" FontSize="14" FontFamily="Segoe UI Light" Padding="30 5 30 5" Content="{x:Static props:Strings.ContinueButton}" Click="btnContinue_Click" />
            <Button x:Name="btnCancel" x:Uid="SetupDeviceButton" IsCancel="True" FontSize="14" FontFamily="Segoe UI Light" Padding="30 5 30 5" Content="{x:Static props:Strings.CancelButton}"  Click="btnCancel_Click" Margin="10,0,0,0" />
        </StackPanel>
        <TextBlock Margin="10,6.457,10,13.543" x:Name="labelMessageText" TextWrapping="Wrap" Text="Message" />
    </DockPanel>
</Window>
