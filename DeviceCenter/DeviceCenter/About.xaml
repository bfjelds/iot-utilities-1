        <TextBlock VerticalAlignment="Top" HorizontalAlignment="Stretch" x:Name="labelClickOnceVersionHeader" Margin="10,90,182,0" FontSize="20" FontFamily="Segoe UI SemiLight" Text="Version:" />
        <TextBlock VerticalAlignment="Top" HorizontalAlignment="Stretch" x:Name="labelClickOnceVersion"  Margin="139,90,10,0" FontFamily="Segoe UI Light" FontSize="20" TextWrapping="Wrap" Text="VersionInfo" Height="27"/>
